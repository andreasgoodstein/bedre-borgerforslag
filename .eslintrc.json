{
    "extends": [
        "eslint:recommended"
    ],
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"        
    },
    "ecmaFeatures": {
        "arrowFunctions": true,
        "classes": true
    },
    "rules": {
        "no-console": "off"
    }
}